{
    "manifest_version": 3,
    "name": "YouTube Video Downloader",
    "version": "1.0",
    "description": "Download YouTube videos by resolution",
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    }
}